//biblioteca 

#include <stdio.h>
#include <locale.h>

int main() {
    int numeros[5]; 

    for (int i = 0; i < 5; i++) {
        printf("Digite o número %d:\n", i + 1);
        scanf("%d", &numeros[i]); 
    }
    printf("Os números inseridos foram:\n");
    for (int i = 0; i < 5; i++) {
        printf("%d ", numeros[i]);
    }

    printf("\n"); 

    return 0;
}
