#include <stdio.h>
#include <locale.h>

int main() {
    setlocale(LC_ALL, "");

    int num[10], i, numComplementar, numEncontrado = -1;

    printf("Insira 10 números inteiros:\n");
    for (i = 0; i < 10; i++) {
        printf("Número %d:\n", i + 1);
        scanf("%d", &num[i]);
    }

    printf("Digite um número para verificar se ele está presente:\n");
    scanf("%d", &numeroComplmentar);

    for (i = 0; i < 10; i++) {
    if (numeros[i] == numeroComplementar) {
    numEncontrado = i;
    break;  
    
    }

    if (numEncontrado != -1) {
        printf("O número %d está presente, na posição %dº.\n", numeroComplementar, numEncontrado);
    } else {
        printf("O número %d não foi encontrado.\n", numEncontrado);
    }

    return 0;
}